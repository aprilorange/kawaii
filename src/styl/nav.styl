.navbar
  clearfix()
  color: white
  text-height: 50px
  a
    color: white
  .brand
    float: left
  &.navbar-default  
    background-color: $headerBgColor
  .nav
    float: left
    list-style: none
    margin-left: 20px
    li
      float: left
      a
        display: block
        padding: 0 15px
        &:hover,&.active
          background-color: $headerBgColor_darker
    &.nav-right
      float: right
